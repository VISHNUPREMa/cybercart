        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Categories </h2>
                    <p> edit   category</p>
                </div>
                
            </div>
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-3">
                           
                            <form action="/admin/category/edit/<%= editCategories.id %>" method="post">
                                <div class="mb-4">
                                    <label for="categoryname" class="form-label">Name</label>
                                    <input type="text" placeholder="Type here" class="form-control" id="categoryname" name="name" value="<%= editCategories.name %>" />
                                </div>
                                <div class="mb-4">
                                    <label for="product_brand" class="form-label">Brand</label>
                                    <input type="text" placeholder="Type here" class="form-control" id="product_brand" name="brand" value="<%= editCategories.brand %>" />
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Description</label>
                                    <textarea placeholder="Type here" class="form-control" name="description"><%= editCategories.description %></textarea>
                                </div>
                                <div class="d-grid">
                                    <button class="btn btn-primary">Edit category</button>
                                </div>
                            </form>
                            
                        </div>
                         <!-- .col// -->
                    </div> <!-- .row // -->
                </div> <!-- card body .// -->

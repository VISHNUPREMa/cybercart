        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Users List</h2>
                <div>
                    <a href="#" class="btn btn-primary"><i class="material-icons md-plus"></i> Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Seller</th>
                                    <th>Email</th>
                                    <th>Status</th>
                                    <th>Registered</th>
                                    <th class="text-end">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% users.forEach(function(user) { %>
                                    <tr>
                                        <td><%= user.name %></td>
                                        <td><%= user.email %></td>
                                        <td>
                                            <% if (user.isBlocked === false) { %>
                                                Active
                                            <% } else { %>
                                                Inactive
                                            <% } %>
                                        </td>
                                        <td><%= user.mobile %></td>
                                        
                                        <td class="text-end">
                                            <a href="/admin/userlist/block/<%= user._id %>"><button class="btn block-btn" id="<%= user._id %>"  style="background-color: #D2E3C8;">Block</button></a>
                                            <a href="/admin/userlist/unblock/<%= user._id %>"><button class="btn unblock-btn" id="<%= user._id %>" style="background-color: #088178;">Unblock</button></a>
                                            
                                        </td>
                                        
                                        
                                        
                                    </tr>
                                <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
                
                <!-- card-body end// -->
            </div> <!-- card end// -->
